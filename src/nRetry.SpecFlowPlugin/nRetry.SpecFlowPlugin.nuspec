<?xml version="1.0"?>
<package>
  <metadata>
    <id>nRetry.SpecFlowPlugin.Calrom</id>
    <version>1.0.0</version>
    <authors>Josh Keegan, Julio C. Tejero</authors>
    <owners>Calrom Ltd.</owners>
    <projectUrl>https://github.com/calrom-jtejero/xRetry.Nunit.Core</projectUrl>
    <license type="expression">MIT</license>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Allows for a @retry(n) tag to be placed above scenarios in SpecFlow.NUnit. This can be used to re-run a failed test up to n times, allowing for transient failures (e.g. network related) to be tolerated. Based on Josh Keegan's xRetry.SpecFlow package</description>
    <language>en-UK</language>
    <copyright>2021</copyright>
    <tags>SpecFlow NUnit Retry Test-Automation</tags>

    <dependencies>
      <dependency id="SpecFlow" version="3.4.8"/>
    </dependencies>
  </metadata>

  <files>
    <file src="nRetry.SpecFlowPlugin.targets" target="build" />
    <file src="nRetry.SpecFlowPlugin.props" target="build" />

    <file src="bin\Release\net472\nRetry.SpecFlowPlugin.dll" target="build\net471" />
    <file src="bin\Release\netstandard2.0\nRetry.SpecFlowPlugin.dll" target="build\netstandard2.0" />
  </files>
</package>